{
  "name": "extension-breaklock",
  "version": "0.1.0",
  "description": "Game to guess pattern locks",
  "scripts": {
    "build": "NODE_ENV=production node ./src/source/src/scripts/build.mjs",
    "build-nominify": "NODE_ENV=development node ./src/source/src/scripts/build.mjs",
    "watch": "npm run build-nominify; wr 'npm run build-nominify' ./src/source/",
    "liveserver": "live-server ./src/source/build",
    "start": "npm run watch & npm run liveserver"
  },
  "author": "EXT Inc.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.2",
    "fs-extra": "^9.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.76.0",
    "wr": "^1.3.1",
    "yaml": "^1.10.0"
  }
}
