{
  "name": "extension-breaklock",
  "version": "0.1.0",
  "description": "Game to guess pattern locks",
  "scripts": {
    "build": "webpack --mode=production --node-env=production --config webpack.config.js",
    "postbuild": "NODE_ENV=production node ./src/source/src/scripts/build.mjs",
    "build-nominify": "NODE_ENV=development node ./src/source/src/scripts/build.mjs",
    "watch": "npm run build-nominify; wr 'npm run build-nominify' ./src/source/",
    "liveserver": "live-server ./src/source/build",
    "start": "npm run watch & npm run liveserver"
  },
  "author": "EXT Inc.",
  "license": "MIT",
  "devDependencies": {
    "@extpkg/types-console": "^1.0.1",
    "@extpkg/types-runtime": "^1.2.1",
    "@extpkg/types-tabs": "^1.1.0",
    "@extpkg/types-websessions": "^1.0.1",
    "@extpkg/types-webviews": "^1.0.2",
    "@extpkg/types-windows": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.48.0",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.2",
    "fs-extra": "^9.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "wr": "^1.3.1",
    "yaml": "^1.10.0"
  }
}
